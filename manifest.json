{
  "name": "Github search",
  "version": "1.1",
  "description": "Github search chrome extension",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Click me!",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
